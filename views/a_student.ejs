<!--教师审批管理页面-->

<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>Insert title here</title>
<%-include('link.ejs')%>

</head>
<body>

	<div class="container-fluid">
		<%-include('a_top.ejs')%>

		<div class="row" style="margin-top: 50px">
			<%-include('a_list.ejs')%>

			<div class="col-lg-9">

				<div class="card ">
				
					<div class="card-body ">
						<table class="table ">
							<thead class="thead text-muted ">
								<tr>
									
									<th scope="col">姓名</th>
									<th scope="col">学号</th>
                                    <th scope="col">密码</th>
									<th scope="col">性别</th>
									<th scope="col">专业</th>
									<th scope="col">班级</th>
									<th scope="col">联系方式</th>
                                    <th scope="col">操作</th>
									
								</tr>
							</thead>
							<tbody>
                                
                                <% for(var i = 0; i < list.length ; i++) { %>
                                 
                                        
                                        
									<tr>
										
										<td><%=list[i].username%></td>
										<td><%=list[i].number%></td>
										<td><%=list[i].password%></td>
										
										<td><%=list[i].sex%></td>
										<td><%=list[i].major%></td>
										<td><%=list[i].class_%></td>
										<td><%=list[i].phone%></td>
										
										<<td><a class="btn btn-outline-primary" href="/delStu?_id=<%=list[i]._id%>" role="button">删除</a></td>
										<<td><a class="btn btn-outline-primary" href="/QueryL?num=<%=list[i].number%>" role="button">请假记录</a></td>


                                        
                                    <%}%>
                                </tr>


								

							</tbody>
						</table>
						
					
				</div>
			</div>
		</div>
	</div>

</body>
</html>