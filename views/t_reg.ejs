<!--教师注册-->
<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>Insert title here</title>
<style type="text/css">
.center-in-center{   
            position: absolute;   
            top: 20%;   
            left: 35%;   
        }  
</style>
<%@ include file="link.jsp"%>
</head>
<body >
	<div class="container-fluid" >

		<%@ include file="top_T.jsp"%>
<div class="row" style="margin-top: 50px">

		<%@ include file="tea_list.jsp"%>
		
		<div class="col-lg-10">

				<div class="card">
					
					<div class="card-body">

						<p style="color: red">${info}</p>

						<form method="post" action="/t_doReg"
							enctype="multipart/form-data">
							<div class="form-group row">
								<label for="inputEmail3" class="col-sm-3 col-form-label">姓名</label>
								<div class="col-sm-5">
									<input type="text" name="username" class="form-control"
										id="inputEmail3" placeholder="姓名">
								</div>
							</div>
							<div class="form-group row">
								<label for="inputEmail3" class="col-sm-3 col-form-label">学号</label>
								<div class="col-sm-5">
									<input type="text" name="number" class="form-control"
										id="inputEmail3" placeholder="学号">
								</div>
							</div>
							<div class="form-group row">
								<label for="inputPassword3" class="col-sm-3 col-form-label">密码</label>
								<div class="col-sm-5">
									<input type="password" name="password" class="form-control"
										id="inputPassword3" placeholder="密码">
								</div>
							</div>
							
							<div class="form-group row">
								<label for="inputEmail3" class="col-sm-3 col-form-label">联系方式</label>
								<div class="col-sm-5">
									<input type="text" name="phone" class="form-control"
										id="inputEmail3" placeholder="联系方式">
								</div>
							</div>

							<div class="form-group row">
								<label for="inputPassword3" class="col-sm-3 col-form-label">性别</label>
								<div class="col-sm-1">
									<div class="form-check form-check-inline">
										<input class="form-check-input" type="radio" name="sex"
											id="sex1" value="男" checked> <label
											class="form-check-label" for="sex1">男</label>
									</div>
								</div>

								<div class="col-sm-1">
									<div class="form-check form-check-inline">
										<input class="form-check-input" type="radio" name="sex"
											id="sex2" value="女" checked> <label
											class="form-check-label" for="sex2">女</label>
									</div>
								</div>
							</div>

							

							<div class="form-group row">
								<label for="inputPassword3" class="col-sm-3 col-form-label">专业</label>
								<div class="col-sm-3">
									<select class="form-control" name="major">
										<option value="电子商务">电子商务</option>
										<option value="信息管理">信息管理</option>
										<option value="软件工程">软件工程</option>
										<option value="人工智能">人工智能</option>
										<option value="计算机">计算机</option>
									</select>
								</div>
							</div>

							
							<div class="form-group row">
								<div class="col-sm-10">
									<button type="submit" class="btn btn-primary">注册</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>

</div>
</body>
</html>